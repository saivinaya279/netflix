      "source": [
        "# spliting data into Train and Test(80:20) (80-train data,20-test data)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 15,
      "metadata": {
        "id": "r4SRWtYnW8es"
      },
      "outputs": [],
      "source": [
        "import os\n",
        "import pandas as pd\n",
        "\n",
        "# Create train.csv if not exists\n",
        "if not os.path.isfile(\"train.csv\"):\n",
        "    df.iloc[:int(df.shape[0] * 0.80)].to_csv(\"train.csv\", index=False)\n",
        "\n",
        "# Create test.csv if not exists\n",
        "if not os.path.isfile(\"test.csv\"):\n",
        "    df.iloc[int(df.shape[0] * 0.80):].to_csv(\"test.csv\", index=False)\n",
        "\n",
        "# Read back the files\n",
        "train_df = pd.read_csv(\"train.csv\", parse_dates=['date'])\n",
        "test_df = pd.read_csv(\"test.csv\")\n"
      ]
    },
