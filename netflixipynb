    {
      "cell_type": "code",
      "source": [
        "# method to make y-axis more redable\n",
        "def human(num,units='M'):\n",
        "    units=units.lower()\n",
        "    num=float(num)\n",
        "    if units=='k':\n",
        "        return str(num/10**3)+\"K\"\n",
        "    elif units=='m':\n",
        "        return str(num/10**6) +\"M\"\n",
        "    elif units =='b':\n",
        "        return str(num/10**9) + \"B\""
      ],
      "metadata": {
        "id": "ieIClygeZQVK"
      },
      "execution_count": 18,
      "outputs": []
    },
